<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>
  <servlet>
   <servlet-name>xyz</servlet-name>
  <servlet-class>calculater.Multi</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>Division</servlet-name>
  	<display-name>Division</display-name>
  	<description></description>
  	<servlet-class>calculater.Division</servlet-class>
  </servlet>
  
  <servlet-mapping>
  <servlet-name>xyz</servlet-name>
  <url-pattern>/multi</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>Division</servlet-name>
  	<url-pattern>/div</url-pattern>
  </servlet-mapping>
  
  <welcome-file-list>
  <welcome-file>DynamicCalci.html</welcome-file>
  </welcome-file-list>
</web-app>
